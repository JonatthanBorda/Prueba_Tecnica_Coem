@model Prueba_Tecnica_Coem.Models.Usuarios


@{
    ViewData["Title"] = "Contacto";
    ViewData["HideNavbar"] = true;
    Layout = "_Layout";
}

<div class="container my-5">
    <div class="row justify-content-center">
        <div class="col-md-8">
            <div class="card shadow-lg">
                <div class="card-body">
                    <h2 class="display-4">Bienvenido a mi prueba técnica</h2>
                    <p class="mt-4">Esta aplicación esta desarrollada en .NET 8.0 utilizando C#, Entity Framework Core 8.0.4 y conectada a una base de datos en SQL Server.</p>
                    <br />
                    <div class="row justify-content-center text-center">
                        <div class="col-12 text-center" style="min-height: 130px;">
                            <img id="Image7" title="VisualStudio" class="icon" src="~/images/ico_visual.png" alt="VisualStudio" style="height:80px;width:80px;">
                            <img id="Image5" title=".NET 8.0" class="icon" src="~/images/ico_net.png" alt="ASP.NET WebForms" style="height:80px;width:80px;">
                            <img id="Image3" title="C#" class="icon" src="~/images/ico_csharp.png" alt="C#" style="height:80px;width:80px;">
                            <img id="Image4" title="Bootstrap" class="icon" src="~/images/ico_bootstrap.png" alt="Bootstrap" style="height:80px;width:80px;">
                            <img id="Image6" title="html5" class="icon" src="~/images/ico_html.png" alt="html5" style="height:80px;width:80px;">
                            <img id="Image8" title="JavaScript" class="icon" src="~/images/ico_js.png" alt="JavaScript" style="height:80px;width:80px;">
                            <img id="Image10" title="SQL Server" class="icon" src="~/images/ico_sql.png" alt="SQL Server" style="height:80px;width:80px;">
                        </div>
                    </div>

                    <h3 class="card-title text-center mb-4">Esta prueba fue realizada por Jonathan Borda Montoya</h3>
                    <p class="mt-4">Gracias por tu atención.</p>
                </div>
            </div>
        </div>
    </div>
</div>